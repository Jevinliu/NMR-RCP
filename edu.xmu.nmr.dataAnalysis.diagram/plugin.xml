<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin
>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="popup:edu.xmu.dataAnalysis.explorer.view?after=additions">
         <command
               commandId="edu.xmu.nmr.dataAnalysis.diagram.commands.opennmreditor"
               id="menu.openwitheditor"
               label="Open With Editor"
               mnemonic="O"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="toolbar:org.eclipse.ui.main.toolbar">
         <toolbar
               id="edu.xmu.nmr.dataAnalysis.diagram.toolbar.diagram"
               label="Diagram">
            <command
                  commandId="edu.xmu.nmr.dataAnalysis.diagram.command.showFidOrSpec"
                  icon="src/resources/images/image.png"
                  id="edu.xmu.nmr.dataAnalysis.diagram.command"
                  label="Diagram"
                  style="pulldown">
               <parameter
                     name="org.eclipse.ui.commands.radioStateParameter"
                     value="Fid">
               </parameter>
            </command>
         </toolbar>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="menu:edu.xmu.nmr.dataAnalysis.diagram.command">
         <command
               commandId="edu.xmu.nmr.dataAnalysis.diagram.command.showFidOrSpec"
               label="Show Fid"
               style="radio">
            <parameter
                  name="org.eclipse.ui.commands.radioStateParameter"
                  value="Fid">
            </parameter>
         </command>
         <command
               commandId="edu.xmu.nmr.dataAnalysis.diagram.command.showFidOrSpec"
               label="Show Spec"
               style="radio">
            <parameter
                  name="org.eclipse.ui.commands.radioStateParameter"
                  value="Spec">
            </parameter>
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="edu.xmu.nmr.dataanalysis.diagram.multieditor.NMRDiagEditor"
            contributorClass="edu.xmu.nmr.dataanalysis.diagram.actions.helper.DAMultiEditorActionBarContributor"
            default="true"
            id="edu.xmu.nmr.dataAnalysis.diagram.nmreditor"
            name="NMR Diagram Editor">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            description="Open the params or fid with this editor"
            id="edu.xmu.nmr.dataAnalysis.diagram.commands.opennmreditor"
            name="Open NMR Editor">
      </command>
      <command
            id="edu.xmu.nmr.dataanalysis.da_move_v_img"
            name="Move Vertical">
      </command>
      <command
            id="edu.xmu.nmr.dataanalysis.da_move_h_img"
            name="Move Horizontal">
      </command>
      <command
            id="edu.xmu.nmr.dataanalysis.zoom_in"
            name="Zoom_In">
      </command>
      <command
            id="edu.xmu.nmr.dataanalysis.zoom_out"
            name="Zoom_Out">
      </command>
      <command
            id="edu.xmu.nmr.dataanalysis.da_part_zoom"
            name="Part Zoom">
      </command>
      <command
            id="edu.xmu.nmr.dataanalysis.da_fft"
            name="FFT">
      </command>
      <command
            id="edu.xmu.nmr.dataanalysis.da_show_full"
            name="Full">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="edu.xmu.nmr.dataanalysis.diagram.handlers.OpenEditorHandler"
            commandId="edu.xmu.nmr.dataAnalysis.diagram.commands.opennmreditor">
         <enabledWhen>
            <reference
                  definitionId="definition.selection.openeditor">
            </reference>
         </enabledWhen>
      </handler>
   </extension>
   <extension
         point="edu.xmu.nmr.custom.extensions.editorpage">
      <editorpage
            class="edu.xmu.nmr.dataanalysis.diagram.multieditor.ParamsEditorPage"
            editorinput="edu.xmu.nmr.dataanalysis.diagram.multieditor.ParamsEditorInput"
            id="edu.xmu.nmr.dataAnalysis.diagram.editorparts.paramseditorpage"
            name="Paramster">
      </editorpage>
      <editorpage
            class="edu.xmu.nmr.dataanalysis.diagram.multieditor.FidEditorPage"
            editorinput="edu.xmu.nmr.dataanalysis.diagram.multieditor.FidEditorInput"
            id="edu.xmu.nmr.dataAnalysis.diagram.editorparts.fidEditorPage"
            name="Fid">
      </editorpage>
      <editorpage
            class="edu.xmu.nmr.dataanalysis.diagram.multieditor.SpecEditorPage"
            editorinput="edu.xmu.nmr.dataanalysis.diagram.multieditor.SpecEditorInput"
            id="edu.xmu.nmr.dataAnalysis.diagram.editorparts.specEditorPage"
            name="Spectrum">
      </editorpage>
   </extension>
   <extension
         point="org.eclipse.core.expressions.definitions">
      <definition
            id="definition.selection.openeditor">
         <with
               variable="org.eclipse.ui.selection">
            <iterate
                  ifEmpty="false"
                  operator="and">
               <or>
                  <instanceof
                        value="edu.xmu.nmr.dataanalysis.logic.explorermodel.FidNode">
                  </instanceof>
                  <instanceof
                        value="edu.xmu.nmr.dataanalysis.logic.explorermodel.ParamtersNode">
                  </instanceof>
               </or>
            </iterate>
         </with>
      </definition>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="edu.xmu.nmr.dataanalysis.diagram.pref.DAPrefCategory"
            id="edu.xmu.nmr.dataAnalysis.diagram.page.dataanalysis"
            name="Data Analysis">
      </page>
      <page
            category="edu.xmu.nmr.dataAnalysis.diagram.page.dataanalysis"
            class="edu.xmu.nmr.dataanalysis.diagram.pref.FidPrefPage"
            id="edu.xmu.nmr.dataAnalysis.diagram.page.fid"
            name="Fid">
      </page>
      <page
            category="edu.xmu.nmr.dataAnalysis.diagram.page.dataanalysis"
            class="edu.xmu.nmr.dataanalysis.diagram.pref.AxisPrefPage"
            id="edu.xmu.nmr.dataAnalysis.diagram.page.axis"
            name="Axis">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="edu.xmu.nmr.dataanalysis.diagram.pref.helper.DAPrefInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <category
            id="edu.xmu.nmr.dataAnalysis.diagram.category.diagram"
            name="Diagram">
      </category>
      <command
            categoryId="edu.xmu.nmr.dataAnalysis.diagram.category.diagram"
            defaultHandler="edu.xmu.nmr.dataanalysis.diagram.handlers.DiagramHandler"
            id="edu.xmu.nmr.dataAnalysis.diagram.command.all"
            name="Diagram">
      </command>
      <command
            categoryId="edu.xmu.nmr.dataAnalysis.diagram.category.diagram"
            defaultHandler="edu.xmu.nmr.dataanalysis.diagram.handlers.ShowDiagHandler"
            id="edu.xmu.nmr.dataAnalysis.diagram.command.showFidOrSpec"
            name="Fid Or Spec">
         <commandParameter
               id="org.eclipse.ui.commands.radioStateParameter"
               name="FidorSpec"
               optional="false">
         </commandParameter>
         <state
               class="org.eclipse.ui.handlers.RadioState:Fid"
               id="org.eclipse.ui.commands.radioState">
            <class
                  class="org.eclipse.ui.handlers.RadioState">
               <parameter
                     name="default"
                     value="Fid">
               </parameter>
               <parameter
                     name="persisted"
                     value="false">
               </parameter>
            </class>
         </state>
      </command>
   </extension>

</plugin>
